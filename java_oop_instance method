public class Student {  //design class
    public String name;
    public int id;//public int id=100;


    public void m1()   //instance variable
    {
        System.out.println("welcome student");
    }

    public void m2(int n)
    {
        System.out.println("welcome student "+ n);
    }


    public void m2(int n,String nam)
    {
        System.out.println("welcome student "+ n + nam);
    }

    public void show_all_instance_variable()
    {
        System.out.println(name +" "+id);
    }

    public static void main(String[] args) {
        System.out.println("welcome");
        Student student1=new Student();//objet1
        student1.name="shakil";
        System.out.println(student1.name);
        student1.id=23;
        System.out.println(student1.id);

        student1.show_all_instance_variable();




        Student student2=new Student();//object2
        student2.name="rafi";
        student1=student2;//referenceing copy betweem to object
        System.out.println(student1.name);
        student1.m1();
        student1.m2(15,"fan");
    }
}
