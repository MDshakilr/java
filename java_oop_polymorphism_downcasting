class Animal {
    public void makaesound() {
        System.out.println("dak");

    }
}
class Dog extends Animal {
    public void makaesound() {
        System.out.println("geo");

    }
    public void bark() {
        System.out.println("bak");

    }
}
    class  Cat extends Animal {
        public void makaesound() {
            System.out.println("meo");

        }
    }



