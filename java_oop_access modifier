        this.section=s;
    }


public void print_details()
{
    System.out.println(name);
    System.out.println(id);
    System.out.println(blood_group);
    System.out.println(section);

}
     public static void main(String[] args) {
        Student s1=new Student("shakil",3,"b+","c");
      s1.print_details();



    }
}





















public class Student {
    public String name;
    private int id;
    public String blood_group;
    public String section;

    public Student(String x, int y, String b, String s) {
        System.out.println("welcome");
        this.name = x;
        this.id = y;
        this.blood_group = b;
        this.section = s;
    }


    public void print_details() {
        System.out.println(name);
        System.out.println(id);
        System.out.println(blood_group);
        System.out.println(section);

    }


        public static void main(String[] args) {
            Student s1 = new Student("shakil", 3, "b+", "c");
            s1.print_details();
            s1.id = 45;//aita Student class i mine same class er viotei tay error hoi ni...jodhi main class ta onno class er vito thakto tahole noton get/set method nia kaj kora lagto 
            s1.print_details();


        }
    }









//aita Student class i mine same class er viotei tay error hoi ni...jodhi main class ta onno class er vito thakto
tahole noton get/set method nia kaj kora lagto 




public class Student {
    public String name;
    private int id;
    public String blood_group;
    public String section;

    public Student(String x, int y, String b, String s) {
        System.out.println("welcome");
        this.name = x;
        this.id = y;
        this.blood_group = b;
        this.section = s;
    }

    // Setter method (private field change করার জন্য)
    public void setId(int newId) {
        this.id = newId;
    }

    // Getter method (private field পড়ার জন্য)
    public int getId() {
        return id;
    }

    public void print_details() {
        System.out.println(name);
        System.out.println(id);
        System.out.println(blood_group);
        System.out.println(section);
        System.out.println("------------------");
    }
}
public class hi {
    public static void main(String[] args) {
        Student s1 = new Student("shakil", 3, "b+", "c");
        s1.print_details();

        // Setter দিয়ে id পরিবর্তন
        s1.setId(45);
        s1.print_details();

        // Getter দিয়ে id দেখা
        System.out.println("Updated ID: " + s1.getId());
    }
}



