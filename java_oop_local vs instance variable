public class Dog {
    public String name;
    public int id;

    public void x(String name,int id)
    {

        this.name=name;
        this.id=id;

    }

   public  static void main(String[] args) {
        Dog d1= new Dog();
        d1.x("doggi",23);

       System.out.println("Name: " + d1.name);
       System.out.println("ID: " + d1.id);

    }
}
