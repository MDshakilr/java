public class Dog {
    public String name="doggi";
    public int id;

    public void x(String name)
    {
this.name=name;
        System.out.println(name);

    }
    public void x(String name,int id)//method overloaading
    {
        this.name=name;
        this.id=id;
        System.out.println(name+" "+id);

}
        public  static void main(String[] args) {
        Dog d1= new Dog();
        d1.x("dog");
        d1.x("bog",34);



    }
}









//sum
public class Dog {
    public int x(int a, int b) {
        int result = a + b;
        return result;
    }
    public int x(int a, int b,int c) {
        int result = a + b+ c;
        return result;
    }
    public static void main(String[] args) {
        Dog d1 = new Dog();
        int sum = d1.x(2, 5,5);
        System.out.println("Sum = " + sum);
        int sum1 = d1.x(2, 5);
        System.out.println("Sum = " + sum1);
    }
}
