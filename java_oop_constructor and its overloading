public class Student {
    public String name;
    public int id;
    public String blood_group;
    public String section;

    public Student(String x,int y,String b,String s){
        System.out.println("welcome");
        this.name=x;
        this.id=y;
        this.blood_group=b;
        this.section=s;
    }
public void print_details()
{
    System.out.println(name);
    System.out.println(id);
    System.out.println(blood_group);
    System.out.println(section);

}
     public static void main(String[] args) {
        Student s1=new Student("shakil",3,"b+","c");
      s1.print_details();
    }
}














//constructor overloading
public class Student {
    public String name;
    public int id;
    public String blood_group;
    public String section;

    public Student(String x,int y,String b,String s){
        System.out.println("welcome");
        this.name=x;
        this.id=y;
        this.blood_group=b;
        this.section=s;
    }
    public Student(String x,int y){
        System.out.println("welcome");
        this.name=x;
        this.id=y;

    }
public void print_details()
{
    System.out.println(name);
    System.out.println(id);
    System.out.println(blood_group);
    System.out.println(section);

}
     public static void main(String[] args) {
        Student s1=new Student("shakil",3,"b+","c");
      s1.print_details();


      Student s2=new Student("rafi",45);
      s2.print_details();
    }
}
