class A{
    public int x=15;
}
class B extends A{
    int x=20;
    public void print()
    {
        int x=50;
        System.out.println("local variable:"+x);
        System.out.println("instance variable:"+this.x);

        System.out.println("super variable from parent class:"+super.x);

    }
}

public class hello{
    public static void main(String[] args) {
      B b1=new B();
      b1.print();

    }
}














class A{
    public static int x=15;
}
class B extends A{
    public static int x=20;
    public void print()
    {
        int x=50;
        System.out.println(x);//loccal/own instance,static/parrentce instance,static
        System.out.println(this.x);//own in,s/p in,s

        System.out.println(super.x);//parentc instance ,static
System.out.println(B.x);//own static/parets static

System.out.println(A.x);//parent static
    }
}

public class hello{
    public static void main(String[] args) {
      B b1=new B();
      b1.print();

    }
}
